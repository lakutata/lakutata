    "@rollup/plugin-typescript": "^11.1.6",
    "@types/crypto-js": "^4.2.2",
    "@types/dockerode": "^3.3.34",
    "@types/is-base64": "^1.1.3",
    "@types/is-glob": "^4.0.4",
    "@types/lodash": "^4.17.17",
    "@types/node": "^20.11.16",
    "@types/object-hash": "^3.0.6",
    "@types/randomstring": "^1.1.12",
    "@types/rollup-plugin-progress": "^1.1.5",
