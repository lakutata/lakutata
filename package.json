{
  "name": "lakutata",
  "version": "2.0.66",
  "description": "An IoC-based universal application framework.",
  "type": "module",
  "exports": {
    ".": "./src/exports/Lakutata.ts",
    "./orm": "./src/exports/ORM.ts",
    "./helper": "./src/exports/Helpers.ts",
    "./com/docker": "./src/exports/DockerComponent.ts",
    "./com/database": "./src/exports/DatabaseComponent.ts",
    "./com/cacher": "./src/exports/CacherComponent.ts",
    "./com/monitor": "./src/exports/MonitorComponent.ts",
    "./com/entrypoint": "./src/exports/EntrypointComponent.ts",
    "./com/logger": "./src/exports/LoggerComponent.ts",
    "./provider/database": "./src/exports/DatabaseProvider.ts",
    "./provider/passwordHash": "./src/exports/PasswordHashProvider.ts",
    "./decorator/asst": "./src/exports/ASSTDecorators.ts",
    "./decorator/ctrl": "./src/exports/CTRLDecorators.ts",
    "./decorator/di": "./src/exports/DIDecorators.ts",
    "./decorator/dto": "./src/exports/DTODecorators.ts",
    "./decorator/orm": "./src/exports/ORMDecorators.ts",
    "./tsconfig.json": "./tsconfig.json",
    "./package.json": "./package.json"
  },
  "scripts": {
    "clean": "shx rm -rf ./distro",
    "build:js": "node --no-warnings --max-old-space-size=32768 rollup.mjs",
    "rebuild": "npm run clean && npm run build:js",
    "prepare": "npm run compile",
    "compile": "npm run rebuild",
    "publish": "cross-env BUILD_MODE=production npm run compile --prod && node publish.mjs",
    "release": "dotenv -- release-it --git.tagExclude='*[-]*' && npm run publish",
    "release:beta-release": "dotenv -- release-it --preRelease=beta && npm run publish",
    "release:alpha-release": "dotenv -- release-it --preRelease=alpha --no-github.release && npm run publish",
    "release:proto-release": "dotenv -- release-it --preRelease=proto --no-github.release --no-git.tag && npm run publish",
    "test:app": "cross-env BUILD_MODE=develop npm run compile && node --trace-warnings distro/src/tests/App.spec.mjs",
    "test:unit": "cross-env BUILD_MODE=develop npm run compile && mocha distro/src/tests/UnitTest.spec.mjs"
  },
  "release-it": {
    "npm": {
      "publish": false
    },
    "git": {
      "requireCleanWorkingDir": true,
      "requireBranch": false,
      "commitMessage": "chore: release v${version}",
      "push": false
    },
    "github": {
      "release": true,
      "autoGenerate": true
    }
  },
  "devDependencies": {
    "@keyv/memcache": "^2.0.2",
    "@keyv/mongo": "^3.0.3",
    "@keyv/mysql": "^2.1.3",
    "@keyv/postgres": "^2.1.6",
    "@keyv/redis": "^4.6.0",
    "@keyv/sqlite": "^4.0.5",
