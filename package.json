    "ansis": "^3.1.0",
    "bcryptjs": "^3.0.2",
    "cache-manager": "^7.0.1",
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "cron-validator": "^1.3.1",
    "cross-env": "^7.0.3",
    "crypto-js": "^4.2.0",
    "degit": "^2.8.4",
    "dockerfile-ast": "^0.7.0",
    "dockerode": "^4.0.8",
